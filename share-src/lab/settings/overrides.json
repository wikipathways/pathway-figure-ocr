{
  "jupyterlab-vimrc:vimrc": {
    "imap": [
      [
        "jk",
        "<Esc>"
      ]
    ]
  },
  "@jupyterlab/apputils-extension:themes": {
    "theme": "base16-gruvbox-dark"
  },
  "@jupyterlab/terminal-extension:plugin": {
    "fontFamily": "Meslo LG S DZ for Powerline,monospace"
  },
  "@jupyterlab/extensionmanager-extension:plugin": {
    "enabled": false
  },
  "@krassowski/jupyterlab-lsp:plugin": {
    "language_servers": {
      "pyls": {
        "serverSettings": {
          "pyls.plugins.pydocstyle.enabled": true,
          "pyls.plugins.pyflakes.enabled": false,
          "pyls.plugins.flake8.enabled": true
        }
      }
    }
  },
  "@ryantam626/jupyterlab_code_formatter:settings": {
    "formatOnSave": true,
    "preferences": {
      "default_formatter": {
        "python": [
          "isort",
          "black"
        ],
        "r": [
          "styler"
        ]
      }
    },
    "black": {
      "line_length": 80,
      "string_normalization": true
    },
    "formatR": {
      "indent": 2,
      "arrow": true,
      "wrap": true,
      "width_cutoff": 80
    }
  }
}
